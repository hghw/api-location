            }).done((xhr) => {
                if (xhr.Succeeded) {
                    swal({
                        title: "Thành công",
                        text: "Đổi mật khẩu thành công!",
                        icon: "success",
                    }).then((result) => {
                        if (result) {
                            window.location.href = '/account/logout';
                        };
                    });
                } else {
                    swal(
                        'Đổi mật khẩu thất bại!',
                        xhr.Errors[0].Description,
                        'error'
                    );
                }
            }).fail(() => {
                swal(
                    'Lỗi!',
                    'Hệ thống gặp lỗi! Vui lòng thử lại sau.',
                    'error',
                );
            });
        })
    </script>
    <!--end::Page Scripts-->
</body>

